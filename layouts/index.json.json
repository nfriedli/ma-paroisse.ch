{{- $index := slice -}}
{{- range .Site.RegularPages }}
  {{- $NPA := apply .Params.NPA "string" "." -}}
  {{- $item := dict
      "title"     .Title
      "territoire" .Params.territoire
      "meta"      .Params.meta
      "canton"    .Params.canton
      "eglise"    .Params.eglise
      "NPA"       $NPA
      "name"      .Params.name
      "region"    .Params.region
      "permalink" .Permalink
      "ministres" .Params.ministres
  -}}
  {{- $index = $index | append $item -}}
{{- end }}
{{- $index | jsonify -}}